# User input (manual)
N = 3
arr = [[2, 1, -1, 8], [-3, -1, 2, -11], [-2, 1, 2, -3]]


# User input's output
print(N)

def print_arr(arr):
  for i in range(N):
    print(arr[i])

print_arr(arr)
